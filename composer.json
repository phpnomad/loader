{
    "name": "phoenix/loader",
    "description": "",
    "type": "library",
    "homepage": "https://github.com/phoenix/core",
    "readme": "README.md",
    "license": "MIT",
    "minimum-stability": "dev",
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rooted-phoenix/tests.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rooted-phoenix/utils.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rooted-phoenix/facade.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phoenix\\Loader\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phoenix\\Loader\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Standiford",
            "email": "alex@standiford.us"
        }
    ],
    "require-dev": {
        "phoenix/tests": "^0.1.0",
        "phoenix/facade": "dev-main",
        "phoenix/utils": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
